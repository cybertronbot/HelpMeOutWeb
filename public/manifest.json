{
  "manifest_version": 3,
  "name": "Help Me Out",
  "version": "2.0",
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab",
    "tabCapture",
    "desktopCapture",
    "https://help-me-out-web-zeta.vercel.app/video-details"
  ], 
  "action": {
    "default_title": "HelpMeOut",
    "default_popup": "./index.html",
    "default_icon": {
        "128": "./images/iCON 7 1.png"
    }
},
  "icons": {
    "128": "./images/iCON 7 1.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
],
 "background": {
        "service_worker": "./background.js"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content.js"]
    }
  ],

"web_accessible_resources": [
    {
        "resources": ["./index.html"],
        "matches": ["<all_urls>"]
    }
]
}